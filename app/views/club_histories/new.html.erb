<h1 class="titleb">Add Your Club History</h1>

<%= simple_form_for([@club_history]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :club_id, collection: Club.pluck(:name,:id) %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :jersey_number %>
    <%= f.input :user_id, :as => :hidden, :input_html => { :value => current_user.id } %>
  <div class="form-actions btn btn-primary">
    <%= f.button :submit, "Add Club History" %>
  </div>
<% end %>
